import { api } from '{{pathToApi}}';

export const {{kebabCase http}}{{pascalName}} = async ({ test }) => {
  const environment = import.meta.env.VITE_ENVIRONMENT;

  if (environment === 'DEV') {
    const response = await fetch(`/mock/{{kebabName}}?test=${test}`);
    const data = await response.json();
    return data;
  }
  
  const response = await api.{{kebabCase http}}(`/{{kebabName}}?test=${test}`);
  const data = response.data;
  return data;
};